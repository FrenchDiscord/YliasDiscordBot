{
  "name": "yliasdiscordbot",
  "version": "4.0.0",
  "description": "Yet another Discord Bot",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "watch": "tsc -p . -w",
    "register-commands-dev": "node dist/command-registration.js",
    "register-commands-prod": "node dist/command-registration.js prod",
    "rcd": "npm run register-commands-dev",
    "rcp": "npm run register-commands-prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LilyWonhalf/YliasDiscordBot.git"
  },
  "keywords": [
    "ylias",
    "discord",
    "bot",
    "discord.js",
    "sapphire"
  ],
  "author": "Lily Wonhalf <lilywonhalf@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LilyWonhalf/YliasDiscordBot/issues"
  },
  "homepage": "https://github.com/LilyWonhalf/YliasDiscordBot#readme",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@lilywonhalf/pretty-logger": "^2.0",
    "@sapphire/fetch": "^1.0.5",
    "@sapphire/framework": "next",
    "@types/node": "^16.4.13",
    "@types/node-fetch": "^2.5.12",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "^3.2.3",
    "@sapphire/prettier-config": "^1.1.6",
    "@sapphire/ts-config": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "jest": "^27.0.6",
    "ts-purify": "^3.0.1",
    "typescript": "^4.3.5",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
