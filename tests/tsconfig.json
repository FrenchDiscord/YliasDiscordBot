{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "noEmit": true,
        "incremental": false,
        "baseUrl": ".",
        "paths": {
            "#mocks/*": ["./mocks/*"],
            "#root/*": ["../src/*"],
            "#root/models/*": ["../src/models/*"],
            "#root/listeners/*": ["../src/listeners/*"],
            "#root/slash-command-preconditions/*": ["../src/slash-command-preconditions/*"],
            "#root/slash-commands/*": ["../src/slash-commands/*"]
        }
    },
    "include": [".", "../src/**/*"],
    "exclude": ["../dist/**/*"],
    "references": [{ "path": "../src" }]
}
